# App Settings
DEBUG=False
SECRET_KEY=your-secret-key-here

# Database Settings
DATABASE_URL=postgresql://user:password@localhost:5432/aroundtrees

# Redis Settings
REDIS_URL=redis://localhost:6379/0

# MapBox Settings
MAPBOX_TOKEN=your-mapbox-token-here

# Data Settings
DATA_DIR=./data

# Celery Settings
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Cache Settings
CACHE_TYPE=redis
CACHE_REDIS_URL=redis://localhost:6379/3
CACHE_DEFAULT_TIMEOUT=600

# Memory Persistence Settings
MEMORY_DATA_DIR=./data/nodes
MEMORY_BACKUP_INTERVAL=3600
MEMORY_GIT_SYNC=true
MEMORY_GIT_BRANCH=memory-backup
MEMORY_AUTO_BACKUP=true

# MCP Server Settings
MCP_SERVER_ENABLED=true
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8811
MCP_SERVER_PROTOCOL=http
